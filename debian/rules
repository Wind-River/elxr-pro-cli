#!/usr/bin/make -f
export DH_VERBOSE=1

%:
	dh $@ --with python3 --buildsystem=pybuild

override_dh_auto_build:
	dh_auto_build

override_dh_auto_test:
	python3 -m flake8 eaclient

override_dh_auto_install:
	dh_auto_install --destdir=debian/elxr-pro

override_dh_auto_clean:
	dh_auto_clean
	make clean

